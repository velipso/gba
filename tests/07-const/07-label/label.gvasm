.base 0
zero:
.i8 1                   /// 01
one:
.im16 1                 /// 01 00
three:

.def add(a, b) = a + b
.i8 add(one, three)     /// 04
.i8 add(one, six)       /// 07
.i8 1                   /// 01
six:

.def a10(a) = a + ten
.i8 a10(1)              /// 0b
.i8 a10(a10(5))         /// 19
.i8 add(a10(2), 1)      /// 0d
.i8                 \
  a10(              \
    add(            \
      a10(          \
        add(        \
          a10(-4),  \
          a10(4)    \
        )           \
      ),            \
      a10(-8)       \
    )               \
  )                     /// 2a

ten:

.def b10(a) = a + ten
.i8 b10(1)              /// 0b
.i8 b10(b10(5))         /// 19
.i8 add(b10(2), 1)      /// 0d
.i8                 \
  b10(              \
    add(            \
      b10(          \
        add(        \
          b10(-4),  \
          b10(4)    \
        )           \
      ),            \
      b10(-8)       \
    )               \
  )                     /// 2a
