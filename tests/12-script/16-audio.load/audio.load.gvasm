.script
  def logData wavData
    var {rate, data} = audio.load wavData
    var out = "rate: $rate, data:"
    for var channel: data
      for var s: channel
        out ~= " ${num.round s * 100}"
      end
      out ~= " ;"
    end
    say out
  end
  logData {
