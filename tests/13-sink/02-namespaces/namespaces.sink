def add x
  say 'bad'
end

namespace foo
  def add a, b
    say "adding $a $b"
    return a + b
  end
end

say (foo.add 4, 5)

def obj.test
  say 'inside obj.test'
end

namespace obj2
  using obj
  test
end

namespace obj3
  using obj
end

obj3.test

# allow using before namespace exists
using qux
def qux.test
  say 'q'
end
test
